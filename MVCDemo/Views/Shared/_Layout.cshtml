<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
</head>
<body>
    <div>
        @await Component.InvokeAsync("LoginLogout")
    </div>
    <div>
        @RenderBody()
    </div>
    <footer>
        @RenderSection("footer", required: false)
        <div>
            <span>@DateTime.Today</span>
        </div>
    </footer>
</body>
</html>
